{
    "name": "vscode-chez",
    "displayName": "vscode-chez",
    "description": "vscode support for chez scheme",
    "version": "0.1.0",
    "publisher": "ch",
    "license": "MIT",
    "icon": "images/lambda.png",
    "engines": {
        "vscode": "^1.25.0"
    },
    "keywords": [
        "scheme",
        "chez"
    ],
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:scheme"
    ],
    "main": "./extension",
    "repository": {
        "type": "git",
        "url": "https://github.com/chclock/vscode-chez.git"
    },
    "bugs": "https://github.com/chclock/vscode-chez/issues",
    "homepage": "https://github.com/chclock/vscode-chez",
    "contributes": {
        "languages": [{
            "id": "scheme",
            "aliases": [
                "Scheme",
                "scheme"
            ],
            "extensions": [
                ".scm",
                ".ss",
                ".sch",
                ".rkt",
                ".sc"
            ],
            "configuration": "./scheme-configuration.json"
        }],
        "grammars": [
            {
                "language": "scheme",
                "scopeName": "source.scheme",
                "path": "./syntaxes/scheme.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "scheme",
                "path": "./snippets/scheme.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}